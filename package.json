{
  "name": "airnote.live",
  "version": "0.1.0",
  "scripts": {
    "install-node": "cd server && npm install",
    "install-svelte": "npm install",
    "setup": "npm run install-node & npm run install-svelte",
    "build": "rollup -c",
    "dev": "npx concurrently --raw \"cd server && npx nodemon server.js\" \"rollup -c -w\"",
    "start": "sirv public --single --port 5555",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "transform": {
      "^.+\\.svelte$": "svelte-jester",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@beyonk/svelte-notifications)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ],
    "testEnvironment": "jsDom"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^28.1.2",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "rollup": "^2.76.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-jester": "^2.3.2"
  },
  "dependencies": {
    "@beyonk/svelte-notifications": "^4.1.0",
    "chart.js": "^3.8.0",
    "date-fns": "^2.28.0",
    "papaparse": "^5.3.1",
    "query-string": "^7.0.0",
    "sirv-cli": "^2.0.2",
    "svelte-routing": "^1.6.0",
    "svelte-speedometer": "^1.1.0"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.0.0"
  }
}
